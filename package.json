{
  "name": "hh-resume-booster",
  "version": "1.2.54",
  "description": "Автоматическое поднятие резюме на HeadHunter",
  "main": "dist/background/serviceWorker.js",
  "scripts": {
    "version:patch": "node scripts/bump-version.js patch",
    "version:minor": "node scripts/bump-version.js minor",
    "version:major": "node scripts/bump-version.js major",
    "build": "npm run version:patch && webpack --mode=production",
    "build:no-version": "webpack --mode=production",
    "dev": "webpack --mode=development --watch",
    "clean": "rm -rf dist",
    "lint": "eslint src/ tests/ --ext .ts,.tsx,.js,.jsx",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "test:unit": "jest --testPathPattern=tests/unit",
    "test:integration": "jest --testPathPattern=tests/integration",
    "test:logical-errors": "jest tests/integration/logicalErrorDetection.test.ts tests/integration/specificLogicalErrors.test.ts tests/integration/logicalErrorFixes.test.ts",
    "test:edge-cases": "jest tests/integration/edgeCaseAndErrorHandling.test.ts tests/integration/errorHandlingImprovements.test.ts",
    "test:performance": "jest tests/integration/performanceAndMemoryAnalysis.test.ts",
    "test:bug-detection": "jest tests/integration/realBugDetection.test.ts tests/integration/realBugHunting.test.ts",
    "test:bug-prevention": "jest tests/integration/bugPreventionTests.test.ts",
    "test:chrome-api": "jest tests/integration/chromeApiCompatibility.test.ts tests/integration/userScenarios.test.ts",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:logical-errors && npm run test:edge-cases && npm run test:performance && npm run test:bug-detection && npm run test:bug-prevention && npm run test:chrome-api",
    "test:e2e": "npm run build && jest --config=jest.e2e.config.js",
    "test:e2e:headless": "npm run build && HEADLESS=true jest --config=jest.e2e.config.js",
    "coverage:badge": "node scripts/generate-coverage-badge.js",
    "coverage:report": "npm run test:coverage && npm run coverage:badge",
    "lint:fix": "eslint src/ tests/ --ext .ts,.tsx,.js,.jsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\" \"tests/**/*.{ts,tsx,js,jsx,json}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,md}\" \"tests/**/*.{ts,tsx,js,jsx,json}\"",
    "lint:ci": "npm run lint && npm run format:check",
    "create-icons": "node scripts/create-custom-icons.js && ./scripts/convert-svg-to-png.sh",
    "prebuild": "npm run create-icons"
  },
  "keywords": [
    "chrome-extension",
    "headhunter",
    "resume",
    "automation",
    "job-search"
  ],
  "author": "Your Name",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.27.1",
    "@babel/preset-env": "^7.27.2",
    "@babel/preset-typescript": "^7.27.1",
    "@eslint/js": "^9.27.0",
    "@types/chrome": "^0.0.237",
    "@types/jest": "^29.5.14",
    "@types/jsdom": "^21.1.7",
    "@types/puppeteer": "^5.4.7",
    "@typescript-eslint/eslint-plugin": "^8.32.1",
    "@typescript-eslint/parser": "^8.32.1",
    "babel-jest": "^29.7.0",
    "copy-webpack-plugin": "^11.0.0",
    "eslint": "^9.27.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.4.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^30.0.0-beta.3",
    "jsdom": "^26.1.0",
    "prettier": "^3.5.3",
    "puppeteer": "^24.9.0",
    "ts-jest": "^29.3.4",
    "ts-loader": "^9.5.2",
    "typescript": "^5.8.3",
    "webpack": "^5.99.9",
    "webpack-cli": "^5.1.4"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yourusername/headhunter-resume-auto-boost.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/headhunter-resume-auto-boost/issues"
  },
  "homepage": "https://github.com/yourusername/headhunter-resume-auto-boost#readme"
}
